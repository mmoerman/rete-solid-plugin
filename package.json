{
  "name": "solid-rete-plugin",
  "version": "0.1.18",
  "description": "A SolidJS plugin for Rete (https://retejs.org/)",
  "scripts": {
    "build": "rm -rf dist && npm run lint && vite build",
    "doc": "rete doc --entries src/index.tsx",
    "prepublishOnly": "npm run build",
    "lint": "eslint"
  },
  "author": "Michael Moerman",
  "license": "MIT",
  "keywords": [
    "solidjs",
    "plugin",
    "rete",
    "Rete.js"
  ],
  "module": "./dist/solid-rete-plugin.mjs",
  "exports": {
    ".": {
      "import": "./dist/solid-rete-plugin.mjs"
    }
  },
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mmoerman/solid-rete-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/mmoerman/solid-rete-plugin/issues"
  },
  "peerDependencies": {
    "rete": "^2.0.0",
    "rete-area-plugin": "^2.0.0",
    "rete-render-utils": "^2.0.0",
    "solid-js": "^1.9.3",
    "solid-styled-components": "^0.28.5"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "globals": "^15.9.0",
    "rete": "^2.0.0",
    "rete-area-plugin": "^2.0.0",
    "rete-render-utils": "^2.0.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-solid": "^2.11.0"
  }
}
